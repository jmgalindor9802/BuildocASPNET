@model Buildoc.Models.Inspeccion

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<div class="row">
    <div class="col-md-12">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group row">
                    <div class="col-md-4">
                        <label asp-for="ProyectoId" class="control-label"></label>
                        <select asp-for="ProyectoId" class="form-control" asp-items="ViewBag.ProyectoId"></select>
                    </div>
                    <div class="col-md-4">
                        <label asp-for="TipoInspeccionId" class="control-label"></label>
                        <select asp-for="TipoInspeccionId" class="form-control" asp-items="ViewBag.TipoInspeccionId"></select>
                    </div>
                    <div class="col-md-4">
                        <label asp-for="FechaInspeccion" class="control-label"></label>
                        <input asp-for="FechaInspeccion" class="form-control" />
                        <span asp-validation-for="FechaInspeccion" class="text-danger"></span>
                    </div>
                </div>
            <div class="form-group">
                <label asp-for="Objetivo" class="control-label"></label>
                <input asp-for="Objetivo" class="form-control" />
                <span asp-validation-for="Objetivo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Descripcion" class="control-label"></label>
                <input asp-for="Descripcion" class="form-control" />
                <span asp-validation-for="Descripcion" class="text-danger"></span>
            </div>
         
          
            <div class="form-group">
                <label asp-for="InspectorId" class="control-label"></label>
                <select asp-for="InspectorId" class ="form-control" asp-items="ViewBag.InspectorId"></select>
            </div>
      
      
        
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
